<script src="/js/common.js"></script>
<div class='error' style='display:none'></div>
<script>
    var app = angular.module('whateverApp', []);
    if (app) {
        app.controller('whateverController', function ($scope, $http) {
            req = JSON.parse('<%- JSON.stringify(req) %>');
            log('init, req = ', req);
            if (toDo) {
                toDo($scope, $http);
            }
        });
    }

    function notify(message) {
        $('.error').text(message).fadeIn(400).delay(3000).fadeOut(400);
    }
</script>