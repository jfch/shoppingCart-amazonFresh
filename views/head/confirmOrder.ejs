	<link href="https://images-na.ssl-images-amazon.com/images/G/01/omaha/css/shoveler._V1397624657_.css" type="text/css" rel="stylesheet">

	<script>
		var forgetWidgetFirstItemId,
			forgetWidgetCurPageNo,
			forgetWidgetItemsPerPage,
			forgetWidgetPageCnt,
			forgetWidgetItemsCnt,
			minWindowWidthFor4ForgetWidgetItems = 1185,
			widgetVisible = false,
			widgetResponse = {},
			widget;
		var WidgetTypes = {
			didYouForgetWidget: {
				id: "#upsellWidget",
				action: "DidYouForgetWidget",
				sources: ["pp"],
				itemsPerPageInBigWindow: 6,
				itemsPerPageInSmallWindow: 6,
				maxItems: 24,
				container : "upsellWidgetBody"
			}
		};
		jQuery(document).ready(function() {
			if(jQuery(WidgetTypes.didYouForgetWidget.id).length > 0) {
				widget = WidgetTypes.didYouForgetWidget;
				populateForgetWidgetItems();
				jQuery(window).resize(arrangeItemsForCurrentWindowSize);
			}
		});
		function logEvent(event) {
			jQuery.post(widget.action + "?event=" + event);
		}
		function populateForgetWidgetItems() {
			var responses = 0, actions = widget.sources;
			jQuery.each(actions, function(i, action) {
				jQuery.get(widget.action+"?action=" + action + getExternalOrderIdParam(), function(ajax) {
					responses++;
					widgetResponse[action] = ajax;
					if (responses == actions.length) {
						populateForgetWidgetItemsFromResponses();
					}
				});
			});
		}
		function getExternalOrderIdParam() {
			var input = jQuery("input[name='externalOrderId']");
			if (input.size() > 0) {
				return "&externalOrderId=" + input.val();
			} else {
				return "";
			}
		}
			function updateFormsForCreateNewOrder() {
			var orderId = jQuery("input[name=externalOrderId]").attr("value");
			var shouldCreateNewOrder = jQuery("input[name=upsellCreateNewOrder]").attr("value");
			if (shouldCreateNewOrder == 'true') {
			jQuery("input[name='orderId']").attr("value", orderId);
			jQuery("input[name='addCartRefer']").attr("value", "ThankYouUpsell");
			jQuery("form.upsell").attr("action", "AddItemToOrder");
			jQuery("form.upsell").attr("onsubmit", "submitOnlyOnce(this)");
			}
		}
		function hideHoverView() {
		jQuery(WidgetTypes.didYouForgetWidget.id + " .hoverOver").hide();
		}
		function populateForgetWidgetItemsFromResponses() {
			switch(widget) {
			case WidgetTypes.didYouForgetWidget:
					attachShovelerWithItems(widgetResponse["pp"]);
					updateFormsForCreateNewOrder();
					break;
			}
			addIdToItemsAndTrim();
			arrangeItemsForCurrentWindowSize();
		}
		function addIdToItemsAndTrim() {
			var items = jQuery(widget.id+" li.forgetWidgetItem"),
				itemCnt = Math.min(widget.maxItems, items.length),
				i;
			for (i = 0; i < itemCnt; i++) {
				jQuery(items.get(i)).attr("id", "forgetWidgetItem_" + i);
			}
			for (var j = i; j < items.length; j++) {
				jQuery(items.get(j)).remove();
			}
		}
		function attachItemsOnly(ajax) {
			var existingItems = jQuery(widget.id+" div.shoveler-content ul"),
			newItems = jQuery(ajax).find("li.forgetWidgetItem");
			existingItems.append(newItems);
		}
		function attachShovelerWithItems(ajax) {
			var widgetObject = jQuery(widget.id),
				box = widgetObject.find("div." + widget.container);
			box.empty();
			box.css("text-align", "left");
			box.append(ajax);
			bindEvents(widgetObject);
		}
		function bindEvents(widgetObject) {
			widgetObject.find("div.back-button a").click(showPrev);
			widgetObject.find("div.next-button a").click(showNext);
		}
		function arrangeItemsForCurrentWindowSize() {
			forgetWidgetFirstItemId = 0;
			forgetWidgetCurPageNo = 0;
			forgetWidgetItemsCnt = jQuery(widget.id+" div.shoveler-cell").length;
			setDisplayAttrsForCurrentWindowSize();
			hideOverflowingItems();
			centerItems();
		}
		function setDisplayAttrsForCurrentWindowSize() {
			if (jQuery(window).width() < minWindowWidthFor4ForgetWidgetItems) {
				forgetWidgetItemsPerPage = widget.itemsPerPageInSmallWindow;
			} else {
				forgetWidgetItemsPerPage = widget.itemsPerPageInBigWindow;
			}
				
			forgetWidgetPageCnt = parseInt(Math.ceil(forgetWidgetItemsCnt / forgetWidgetItemsPerPage));
			jQuery(widget.id+" span.pageCnt").empty().append(forgetWidgetPageCnt);
			jQuery(widget.id+" span.currentPage").empty().append(forgetWidgetCurPageNo + 1);
		}
		function centerItems() {
			if(widget == WidgetTypes.didYouForgetWidget) {
				var containerWidth = jQuery(widget.id+" .shoveler-content").width()-100;
				var cellWidth = jQuery(widget.id+" .shoveler-cell:first").width();
				var emptySpace = containerWidth - (cellWidth * forgetWidgetItemsPerPage);
				var spacing = emptySpace / (forgetWidgetItemsPerPage - 1);
				for(var i = 0; i < forgetWidgetItemsCnt; i++) {
					var hIndex = i % forgetWidgetItemsPerPage;
					var left = hIndex * (cellWidth + spacing) + 50;
					jQuery("#forgetWidgetItem_" + i).css("left", left+"px");
				}
			}
		}
		function hideOverflowingItems() {
			jQuery("li.forgetWidgetItem").show();
			for (var i = forgetWidgetFirstItemId + forgetWidgetItemsPerPage; i < forgetWidgetItemsCnt; i++)
				jQuery("#forgetWidgetItem_" + i).hide();
		}
		function showPrev() {
			forgetWidgetCurPageNo += -1 + forgetWidgetPageCnt;
			forgetWidgetCurPageNo %= forgetWidgetPageCnt;
			scrollWidgetContents();
		}
		function showNext() {
			forgetWidgetCurPageNo++;
			forgetWidgetCurPageNo %= forgetWidgetPageCnt;
			scrollWidgetContents();
		}
		function scrollWidgetContents() {
			for (var i = forgetWidgetFirstItemId; i < forgetWidgetFirstItemId + forgetWidgetItemsPerPage; i++) {
				jQuery("#forgetWidgetItem_" + i).hide();
			}
			jQuery(widget.id+" span.currentPage").empty().append(forgetWidgetCurPageNo + 1);
			forgetWidgetFirstItemId = forgetWidgetCurPageNo * forgetWidgetItemsPerPage;
			for (var i = forgetWidgetFirstItemId; i < forgetWidgetFirstItemId + forgetWidgetItemsPerPage; i++) {
				jQuery("#forgetWidgetItem_" + i).show();
			}
		}
		</script>

